<AnchorPane id="AnchorPane" prefHeight="526.0" prefWidth="681.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ifg.controller.FXMLCadastrarMedicoController">
